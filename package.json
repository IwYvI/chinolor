{
  "name": "chinolor",
  "displayName": "Chinolor Theme",
  "description": "A VSCode theme based on Chinese traditional color.",
  "version": "0.2.11",
  "publisher": "iwyvi",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IwYvI/chinolor"
  },
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Chinolor",
        "uiTheme": "vs-dark",
        "path": "./themes/Chinolor-color-theme.json"
      },
      {
        "label": "Chinolor Light",
        "uiTheme": "vs",
        "path": "./themes/Chinolor-light-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1"
  }
}
